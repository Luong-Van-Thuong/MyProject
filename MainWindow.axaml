<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:browser="clr-namespace:MyProject.Browser"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="MyProject.MainWindow"
        Title="MyProject" Closed="Window_Closed" Icon="wwwroot/favicon.png">
    <browser:BlazorBrowserView x:Name="BrowserView" HostPage="wwwroot/index.html">
        <browser:BlazorBrowserView.RootComponents>
           <browser:RootComponent Selector="#app" ComponentType="MyProject.Main" />
        </browser:BlazorBrowserView.RootComponents>
    </browser:BlazorBrowserView>
</Window>
